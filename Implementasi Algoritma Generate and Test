    {
      "cell_type": "code",
      "source": [
        "#\n",
        "# PENJADWALAN MK & LAB GENERATE AND TEST\n",
        "#\n",
        "# Tujuan: menghasilkan semua jadwal yang valid,\n",
        "# lalu (opsional) meranking berdasarkan preferensi.\n",
        "import itertools\n",
        "from rich.console import Console\n",
        "from rich.table import Table\n",
        "\n",
        "# Data dasar\n",
        "labs = [\"Lab AI\", \"Lab IOT\", \"Lab Multimedia\"]\n",
        "slots = [\"Pagi\", \"Siang\", \"Sore\"]\n",
        "mks = [\"Kecerdasan Buatan\", \"Pemrograman Dasar\", \"Jaringan Komputer\"]\n",
        "\n",
        "# Aturan kesesuaian lab per MK (hard constraints)\n",
        "allowed_labs = {\n",
        "    \"Kecerdasan Buatan\": [\"Lab AI\"],  # wajib Lab AI\n",
        "    \"Pemrograman Dasar\": [\"Lab AI\", \"Lab Multimedia\"], # bukan Lab IOT\n",
        "    \"Jaringan Komputer\": [\"Lab AI\", \"Lab IOT\"] # bukan Lab Multimedia\n",
        "}\n",
        "\n",
        "#--- Larangan khusus (mis. maintenance) ---\n",
        "# Format: himpunan pasangan (lab, slot) yang TIDAK boleh dipakai\n",
        "forbidden_lab_slots = {\n",
        "    (\"Lab AI\", \"Sore\") # contoh: Lab AI sedang maintenance di slot Sore\n",
        "}\n",
        "\n",
        "# --- Soft constraint (opsional): preferensi untuk ranking hasil\n",
        "def preference_score(assignment):\n",
        "    \"\"\"\n",
        "    assignment: dict MK -> (lab, slot)\n",
        "    Poin +1 jika Jaringan Komputer ditempatkan di Lab IOT (preferensi).\n",
        "    Bisa ditambah preferensi lain sesuai kebutuhan.\n",
        "    \"\"\"\n",
        "    score = 0\n",
        "    lab_jarkom = assignment[\"Jaringan Komputer\"][0]\n",
        "    if lab_jarkom == \"Lab IOT\":\n",
        "        score += 1\n",
        "    return score\n",
        "\n",
        "#--- Fungsi TEST untuk satu jadwal\n",
        "def valid_schedule(assignment):\n",
        "    \"\"\"\n",
        "    assignment: dict MK -> (lab, slot)\n",
        "    Cek semua hard constraints:\n",
        "    - unik per lab dan per slot\n",
        "    - lab sesuai allowed labs\n",
        "    - (lab, slot) tidak termasuk forbidden\n",
        "    \"\"\"\n",
        "    # 1) Cek kesesuaian lab terhadap allowed_labs\n",
        "    for mk, (lab, slot) in assignment.items():\n",
        "        if lab not in allowed_labs[mk]:\n",
        "            return False\n",
        "\n",
        "    # 2) Cek larangan (lab, slot) karena maintenance dsb.\n",
        "    for mk, (lab, slot) in assignment.items():\n",
        "        if (lab, slot) in forbidden_lab_slots:\n",
        "            return False\n",
        "\n",
        "    # 3) Tidak boleh bentrok lab pada slot yang sama dan tidak boleh dua MK pada slot yang sama\n",
        "    used_labs = set()\n",
        "    used_slots = set()\n",
        "    for mk, (lab, slot) in assignment.items():\n",
        "        if lab in used_labs: #lab sudah dipakai MK lain\n",
        "            return False\n",
        "        if slot in used_slots: # slot sudah dipakai MK lain\n",
        "            return False\n",
        "        used_labs.add(lab)\n",
        "        used_slots.add(slot)\n",
        "\n",
        "    # Lolos semua cek valid\n",
        "    return True\n",
        "\n",
        "#\n",
        "# GENERATE & TEST\n",
        "#\n",
        "valid_schedules = []\n",
        "\n",
        "# Langkah:\n",
        "# 1) Bentuk semua kombinasi pilihan lab legal untuk 3 MK.\n",
        "# 2) Untuk setiap kombinasi lab, pasangkan dengan semua permutasi slot (Pagi, Siang, Sore).\n",
        "# 3) Uji valid_schedule().\n",
        "\n",
        "# 1) Semua kombinasi lab legal per urutan mks\n",
        "legal_lab_lists = [allowed_labs[mk] for mk in mks]\n",
        "for lab_choice in itertools.product(*legal_lab_lists):\n",
        "    # lab_choice contoh: ('Lab AI', 'Lab Multimedia', 'Lab IOT')\n",
        "\n",
        "    # 2) Semua permutasi slot untuk 3 MK (agar tidak ada slot ganda)\n",
        "    for slot_choice in itertools.permutations(slots, len(mks)):\n",
        "\n",
        "        # Bentuk assignment: MK -> (lab, slot)\n",
        "        assignment = {\n",
        "            mk: (lab_choice[i], slot_choice[i]) for i, mk in enumerate(mks)\n",
        "        }\n",
        "\n",
        "        # TEST aturan\n",
        "        if valid_schedule(assignment):\n",
        "            # Hitung skor preferensi (semakin besar semakin diprioritaskan)\n",
        "            score = preference_score(assignment)\n",
        "            valid_schedules.append((score, assignment))\n",
        "\n",
        "# Tampilkan hasil\n",
        "console = Console()\n",
        "valid_schedules.sort(key=lambda x: x[0], reverse=True)\n",
        "if not valid_schedules:\n",
        "    console.print(\"[bold red] Tidak ada jadwal valid yang memenuhi semua aturan.[/bold red]\")\n",
        "else:\n",
        "    console.print(f\"[bold green] Jumlah jadwal valid: {len(valid_schedules)} [/bold green]\\n\")\n",
        "    console.print(\"[bold cyan] Contoh hingga 10 jadwal teratas (berdasarkan preferensi): [/bold cyan]\\n\")\n",
        "    # buat tabel\n",
        "    table = Table(title=\"Jadwal MK & Lab (Top 10)\", show_lines=True)\n",
        "    table.add_column(\"Rank\", style=\"bold magenta\")\n",
        "    table.add_column(\"Skor\", style=\"bold yellow\")\n",
        "    table.add_column(\"Mata Kuliah\", style=\"bold cyan\")\n",
        "    table.add_column(\"Lab\", style=\"bold green\")\n",
        "    table.add_column(\"Slot\", style=\"bold blue\")\n",
        "\n",
        "    # isi tabel\n",
        "    for rank, (score, assignment) in enumerate(valid_schedules[:10], start=1):\n",
        "        for i, mk in enumerate(mks):\n",
        "            lab, slot = assignment[mk]\n",
        "            if i == 0:\n",
        "                table.add_row(str(rank), str(score), mk, lab, slot)\n",
        "            else:\n",
        "                table.add_row(\"\", \"\", mk, lab, slot)\n",
        "        table.add_row(\"\", \"\", \"\", \"\", \"\")  # baris kosong pemisah\n",
        "    console.print(table)\n"
      ],
